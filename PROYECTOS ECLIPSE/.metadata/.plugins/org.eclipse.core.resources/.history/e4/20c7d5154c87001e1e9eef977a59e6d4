import java.util.*;
public class Practica_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner leer = new Scanner(System.in);
		
		System.out.println("Ingresa en numeros tu edad.");
		
		int edad = leer.nextInt();
		
		if (edad>=18) {
			
			System.out.println("Puedes Votar en las proximas elecciones. ");
		}
		else {
			
			System.out.println("No puedes Votar en las proximas elecciones. ");
			
			leer.close();
		}
		 
		
		
	}

}
