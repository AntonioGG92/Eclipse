import java.util.Scanner;

public class Ejercicio2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		 Scanner leer = new Scanner(System.in);
		 
		 int fibonacci=leer.nextInt();
		 int numero1=1;
		 int numero2=1;
		 int numero3;
	       
		 System.out.println(" 01)    1  ");
		 for(int i=1;i<fibonacci-1;i++) {
			 System.out.printf(" %02d)",i+2);
			 System.out.printf("% 7d  ",numero1);
			 numero3=numero1;
			 numero1=numero1+numero2;
			 numero2=numero3;
			 if((i+2)%10==0 & i!=0) {
			 System.out.println();
			 
			 leer.close();
		 }
	   }
	}
}